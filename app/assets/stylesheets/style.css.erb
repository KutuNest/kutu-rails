/*loader*/

.pageload {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  background-color: #F15152;
  background-color: rgba(241, 81, 83, 0.99);
  z-index: 99999;
}

.pageload .text-loader {
  color: #fff;
  position: absolute;
  margin: 28px 0 0 -36px;
  top: 50%;
  left: 50%;
  text-align: center;
  width: 75px;
  font-size: .9em;
  display: inline-block;
}

.pageload .loader {
  position: absolute;
  margin: -12px 0 0 -21px;
  top: 50%;
  left: 50%;
  text-align: center;
  width: 40px;
}

.loader:after {
  /*border-radius: 50%;*/
  background: url(<%= asset_url 'logo.svg' %>);
  background-size: 40px 38px;
  background-repeat: no-repeat;
  height: 38px;
  width: 40px;
  display: inline-block;
  margin: 0.3rem 0;
  width: 40px;
  height: 38px;
  margin: 0 auto;
  display: block;
  z-index: 2000;
  /*border-radius: 24px;*/
  content: "";
  -webkit-animation: spinner 900ms linear infinite;
  -moz-animation: spinner 900ms linear infinite;
  -ms-animation: spinner 900ms linear infinite;
  -o-animation: spinner 900ms linear infinite;
  animation: spinner 900ms linear infinite;
}

/*.loader:after {
    border: solid 1px transparent;
    border-top-color: #1582dc;
    border-left-color: #1582dc;
}*/

@-webkit-keyframes spinner {
  0% {
    -webkit-transform: rotate(0deg);
    transform: rotate(0deg);
  }
  100% {
    -webkit-transform: rotate(360deg);
    transform: rotate(360deg);
  }
}
@-moz-keyframes spinner {
  0% {
    -moz-transform: rotate(0deg);
    transform: rotate(0deg);
  }
  100% {
    -moz-transform: rotate(360deg);
    transform: rotate(360deg);
  }
}
@-o-keyframes spinner {
  0% {
    -o-transform: rotate(0deg);
    transform: rotate(0deg);
  }
  100% {
    -o-transform: rotate(360deg);
    transform: rotate(360deg);
  }
}
@-ms-keyframes spinner {
  0% {
    -ms-transform: rotate(0deg);
    transform: rotate(0deg);
  }
  100% {
    -ms-transform: rotate(360deg);
    transform: rotate(360deg);
  }
}
@keyframes spinner {
  0% {
    transform: rotate(0deg);
  }
  100% {
    transform: rotate(360deg);
  }
}


