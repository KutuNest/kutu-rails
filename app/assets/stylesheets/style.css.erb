    color: #414141;
    display: block;
    padding: 0.75rem 0 0.75rem 2rem;
    margin-bottom: -1px;
    width: 100%;
    position: relative;
}

.list-group-item i{
  position: absolute;
  left: 0;
  margin-left: .5rem;
  top: calc(50% - 8px);
}

.list-group-item.dropdown i{
  margin-left: -1.5rem;
} 
.list-group-item:first-child {
    border-top-right-radius: 0;
    border-top-left-radius: 0;
