<div class="container padding-top-90">
  <div class="row">
    <div class="col-md-12">

        <%= render 'layouts/notice' %>

        <h1 class="title block">Complete Bank Information</h1>

        <%= form_for(@member, url: update_member_path(@member), html: { method: :post }) do |f| %>

          <%= render 'member/form', f: f %>

          <div class="actions">
            <%= f.submit "Complete", class: 'btn btn-primary' %>
          </div>   

        <% end %>

        <br/>

    </div>
  </div>
  <hr/>
</div>

<%- page_title 'Complete Bank Information' %>