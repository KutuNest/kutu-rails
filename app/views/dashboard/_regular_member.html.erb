    <section class="content-section dashboard-public">
      <div class="container paddingBottom-10">
        <div class="row flex cards marginTop-10">
            <div class="col-xs-12 col-md-6 col-lg-3">
              <div class="row flex tiles-container tiles-border h-full">
                <div class="col-md-4 col-xs-5 no-padding h-full">
                  <div class="tiles blue h-full content-center">
                    <i class="fa fa-chevron-circle-down"></i>
                  </div>
                </div>
                <div class="col-md-8 col-xs-7 tiles white p-10 text-center">
                  <h3 class="text-uppercase text-bold"><%= number_to_currency(current_member.summary[:money_received]) %></h3>
                  <small class="text-uppercase">Total Money</small><br/>
                  <small class="text-uppercase">Received</small>
                </div>
              </div>
            </div>
            
            <div class="col-xs-12 col-md-6 col-lg-3">
              <div class="row flex tiles-container tiles-border h-full">
                <div class="col-md-4 col-xs-5 no-padding h-full">
                  <div class="tiles purple h-full content-center">
                    <i class="fa fa-chevron-circle-up"></i>
                  </div>
                </div>
                <div class="col-md-8 col-xs-7 tiles white p-10 text-center">
                  <h3 class="text-uppercase text-bold">
                    <%= number_to_currency(current_member.summary[:money_sent]) %>
                  </h3>
                  <small class="text-uppercase">Total Money</small><br/>
                  <small class="text-uppercase">Sent</small>
                </div>
              </div>
            </div>
            
            <div class="col-xs-12 col-md-6 col-lg-3">
              <div class="row flex tiles-container tiles-border h-full">
                <div class="col-md-4 col-xs-5 no-padding h-full">
                  <div class="tiles orange h-full content-center">
                    <i class="fa fa-exchange"></i>
                  </div>
                </div>
                <div class="col-md-8 col-xs-7 tiles white p-10 text-center">
                  <h3 class="text-uppercase text-bold">
                    <%= current_member.summary[:total_transaction] %>
                    <small>&nbsp; TRX's</small>
                  </h3>
                  <small class="text-uppercase"><%= current_member.summary[:pending_transaction] %> PENDING</small><br/>
                  <small class="text-uppercase"><%= current_member.summary[:failed_transaction] %> FAILED</small>
                </div>
              </div>
            </div>
            
            <div class="col-xs-12 col-md-6 col-lg-3">
              <div class="row flex tiles-container tiles-border h-full">
                <div class="col-md-4 col-xs-5 no-padding h-full">
                  <div class="tiles red h-full content-center">
                    <i class="fa fa-user"></i>
                  </div>
                </div>
                <div class="col-md-8 col-xs-7 tiles white p-10 text-center">
                  <h3 class="text-uppercase text-bold">
                    <%= current_member.summary[:total_accounts] %>
                    <small>&nbsp; Accounts</small>
                  </h3>
                  <small class="text-uppercase"><%= current_member.summary[:active_accounts] %> ACTIVE</small><br/>
                  <small class="text-uppercase"><%= current_member.summary[:inactive_accounts] %> INACTIVE</small>
                </div>
              </div>
            </div>

        </div>
      </div>
      <div class="container paddingTop-10 paddingBottom-10">
        <div class="row no-gutters marginTop-10">


          <div class="col-md-12 m-b-10">
            <div class="widget-item ">
              <div class="controller overlay right">
                <a href="javascript:;" class="reload"></a>
                <a href="javascript:;" class="remove"></a>
              </div>
              <div class="tiles green " style="max-height:345px">
                <div class="tiles-body">
                  <h3 class="text-white m-t-50 m-b-30 m-r-20"> <span class="semi-bold"><%= distance_of_time_in_words(DateTime.now, (DateTime.now + @current_transaction.timeout.seconds)) %> </span> to send <span class="semi-bold"><%= number_to_currency @current_transaction.value %></span> </h3>
                  <div class="overlayer bottom-right fullwidth">
                    <div class="overlayer-wrapper">
                      <div class=" p-l-20 p-r-20 p-b-20 p-t-20">
                        <div class="pull-right"> <a href="#" class="hashtags transparent unrounded"> Send Money </a> </div>
                        <div class="clearfix"></div>
                      </div>
                    </div>
                  </div>
                  <br>
                </div>
              </div>
              <div class="tiles white ">
                <div class="tiles-body">
                  <div class="row">
                    <div class="user-comment-wrapper pull-left user-comment-wrapper-padding ">
                      <div class="profile-wrapper"> 
                        
                      </div>
                      <div class="comment">
                        <div class="preview-wrapper">Transaction timeout at</div>
                        <div class="user-name text-black bold"> <%= (@current_transaction.created_at + @current_transaction.timeout.seconds).to_s(:short) %> </div>
                      </div>
                      <div class="clearfix"></div>
                    </div>
                    <div class="pull-right m-r-20"> <span class="bold text-black small-text"></span> </div>
                    <div class="clearfix"></div>
                    <div class="p-l-15 p-t-10 p-r-20">
                      <p>
                        <%= @current_transaction.eater.member.bank.title %> <br/>
                        <%= @current_transaction.eater.member.account_number %> <br/>
                        <%= @current_transaction.eater.member.account_holder_name %>
                      </p>
                      <div class="post p-t-10 p-b-10">
                        <ul class="action-bar no-margin p-b-20 ">
                          <li><a href="#" class="muted bold"><i class="fa fa-phone  m-r-10"></i><%= @current_transaction.eater.member.phone_number %></a> </li>
                        </ul>
                        <div class="clearfix"></div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>


          <div class="col-12">
            <dl class="accordion">
              <dt href="#accordion1" aria-expanded="true" aria-controls="accordion2" class="js-accordionTrigger">
                <span class="icon-square bg-primary text-white">
                  <i class="fa fa-exchange"></i>
                </span>
                  <a href="" class="is-expanded">COMPLETED TRANSACTIONS</a>
              </dt>
              <dd class="accordion-content accordionItem is-expanded" id="accordion2" aria-hidden="false">
                <div class="row no-gutters">
                  <div class="col-12 col-lg-12">
                    <div class="table-responsive">
                      <table class="table table-hover table-dash ">
                        <thead class="">
                          <tr>
                            <th>Completed Date</th>
                            <th>Name</th>
                            <th>Bank Account</th>
                            <th>Account Name</th>
                            <th>Amount</th>
                            <th>Phone Number</th>
                            <th></th>
                          </tr>
                        </thead>
                        <tbody>
                          <%- for t in @transaction_history do %>
                          <tr>
                            <td><%= t.completed_date %></td>
                            <td><%= t.eater.member.full_name %></td>
                            <td><%= t.eater.member.bank.try(:title).to_s + " " + t.eater.member.account_number.to_s %></td>
                            <td><%= t.eater.member.account_holder_name %></td>
                            <td><label class="label label-success"><%= number_to_currency t.value %></label></td>
                            <td><%= t.eater.member.phone_number %></td>
                            <td><a class="btn btn-info btn-sm" href="<%= transaction_path(t) %>"><i class="fa fa-search-plus "></i> show</a></td>
                          </tr>
                          <%- end %>
                        </tbody>
                      </table>
                    </div>
                  </div>
                </div>
              </dd>
            </dl>
          </div>
        </div>
      </div>
    </section>